      customerid:"<?php echo $customer->getId() ?>"
    },
    success: function(response){
    	result=JSON.parse(response);    	
    	var finallog = (result.graphdata.tgresult).toLowerCase();
    	jQuery(".loader").css("display", "none");
    	jQuery(".tgerror").text("Thank you for information");
    	jQuery(".tgerror").css("color","#5cb85c"); 
		jQuery(".tgerror").css("display","block");
		jQuery(".tgdata").text(tgvalue); 
		jQuery('.tgpanel').removeClass('panel-green panel-red');
		jQuery(".tgresult").text(result.graphdata.tglabel);		
    	if( finallog == 'not good'){
