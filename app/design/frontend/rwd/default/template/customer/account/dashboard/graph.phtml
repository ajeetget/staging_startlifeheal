 //document.getElementById(cityName).style.display = "block";
  evt.currentTarget.className += " active";
}

// Get the element with id="defaultOpen" and click on it
document.getElementById("defaultOpen").click();

var jq = $.noConflict();
jq(function(){
              jq('.showSingle').click(function(){
			    //remove all pre-existing active classes
              jq('.active-tab').removeClass('active-tab'); 
              //add the active class to the link we clicked
              jq(this).addClass('active-tab');
              jq('.targetDiv').hide();  
			  
			  jq('#div'+jq(this).attr('target')).show();
        });
});
  jq(".toggle-accordion").on("click", function() {
    var accordionId = jq(this).attr("accordion-id"),
      numPanelOpen = jq(accordionId + ' .collapse.in').length;
    
    jq(this).toggleClass("active");

    if (numPanelOpen == 0) {
      openAllPanels(accordionId);
    } else {
      closeAllPanels(accordionId);
    }
  })

  openAllPanels = function(aId) {
    console.log("setAllPanelOpen");
    jq(aId + ' .panel-collapse:not(".in")').collapse('show');
	
  }
  closeAllPanels = function(aId) {
    console.log("setAllPanelclose");
   jq(aId + ' .panel-collapse.in').collapse('hide');
  }
  

jq(function() {

    var jqallVideos = jq("iframe[src^='//player.vimeo.com'], iframe[src^='//www.youtube.com'], object, embed"),
   jqfluidEl = jq("figure");

	jqallVideos.each(function() {

	  jq(this)
	    // jQuery .data does not work on object/embed elements
	    .attr('data-aspectRatio', this.height / this.width)
	    .removeAttr('height')
	    .removeAttr('width');

	});

	jq(window).resize(function() {

	  var newWidth = jqfluidEl.width();
	 jqallVideos.each(function() {

	    var jqel = jq(this);
	    jqel
	        .width(newWidth)
	        .height(newWidth * jqel.attr('data-aspectRatio'));

	  });

	}).resize();

});

function SaveBPData(){
jQuery(".loader").css("display", "block"); 
jQuery(".bperror").css("display","none");
var upperbp = jQuery("#high").val();
var lowerbp = jQuery("#Low").val();
if(upperbp && lowerbp){
jQuery.ajax({
    url: "<?php echo Mage::getBaseUrl().'weeklyhealthcheckin/index/insetBpRecords' ?>",
    type: "post",
    data: {
      mobile : "<?php echo $customer->getPrimarymobileno() ?>",
      name : "<?php echo $customer->getName() ?>",
      email: "<?php echo $customer->getEmail() ?>",
      lowerbp: lowerbp,  
      upperbp: upperbp,
      customerid:"<?php echo $customer->getId() ?>"
    },
    success: function(response){
		result=JSON.parse(response);    	
    	var finallog = (result.graphdata.bpresult).toLowerCase();
    	jQuery(".loader").css("display", "none"); 
    	jQuery(".bperror").text("Thank you for information");
    	jQuery(".bperror").css("color","#5cb85c"); 
		jQuery(".bperror").css("display","block"); 
		jQuery(".bpdata").text(upperbp+'/'+lowerbp);
		jQuery('.bppanel').removeClass('panel-green panel-red');
		jQuery(".bpresult").text(result.graphdata.bplabel);		
    	if( finallog == 'not good'){
    	jQuery('.bppanel').addClass('panel-red');
    	}else { jQuery('.bppanel').addClass('panel-green'); }
    }
});
}else{ 
	jQuery(".bperror").text( "Please enter Lower and Upper BP values");
	jQuery(".bperror").css("display","block");
	jQuery(".loader").css("display", "none");
	}
}
